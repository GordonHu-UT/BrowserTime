{
  "manifest_version": 2,
  "name": "BrowserTime",
  "short_name": "BrowserTime",
  "author": "Sean Miller",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "history",
    "storage",
    "topSites",
    "tabs",
    "background",
    "debugger",
    "webRequests",
    "webRequestBlocking",
    "*://*/*"
  ],
  "chrome_url_overrides": {
    "history": "history.html"
  }
}
